<h1>Searching for - <%= @search_word %></h1>
<table border="1">
  <tr>
    <th>Image</th>
    <th>Name></th>
    <th>Link to recipe</th>
  </tr>
  <% @recipe.each do |recipes| %>
  <tr>
    <td><%= image_tag(recipes["image_url"])%></td>
    <td><%= recipes["title"] %></td>
    <td><%= link_to('recipe', recipes["f2f_url"])%></td>
  </tr>
  <% end %>
</table>
<h1>Recipe#index</h1>
<p>Find me in app/views/recipe/index.html.erb</p>
